<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2011/08/nuspec.xsd">
    <metadata>
        <id>Plugin.GAXamarin</id>
        <version>1.0.3</version>
        <title>Google Analytics Plugin for Xamarin and Windows</title>
        <authors>Alex Rainman</authors>
        <owners>Alex Rainman</owners>
        <licenseUrl>https://github.com/alexrainman/GAXamarin/blob/master/README.md</licenseUrl>
        <projectUrl>https://github.com/alexrainman/GAXamarin</projectUrl>
        <requireLicenseAcceptance>false</requireLicenseAcceptance>
        <summary>Google Analytics Plugin for Xamarin and Windows</summary>
        <description>Standard API to track Users, Screens, Events, Timing, Exceptions, Custom Dimensions and Metrics from PCL.</description>
        <releaseNotes>
        1.0.3
        [fix] Adding support to track unhandled exceptions
        1.0.2
        [fix] Adding platform specific dependencies
        1.0.1
        [enhancement] Adding an extra parameter to Init method to enable/disable reporting uncaught exceptions.
        </releaseNotes>
        <tags>google analytics ios android uwp wp81 winstore xamarin forms monotouch monodroid</tags>
        <dependencies>
            <group targetFramework="MonoAndroid10">
	            <dependency id="Xamarin.GooglePlayServices.Analytics " version="29.0.0.2"/>
	        </group>
	        <group targetFramework="Xamarin.iOS10">
	            <dependency id="Xamarin.Google.iOS.Analytics" version="3.14.0.8"/>
	        </group>
	        <group targetFramework="UAP10">
	            <dependency id="GoogleAnalyticsSDK" version="1.3.0"/>
	        </group>
	        <group targetFramework="wp8">
	            <dependency id="GoogleAnalyticsSDK" version="1.3.0"/>
	        </group>
	        <group targetFramework="wpa81">
	            <dependency id="GoogleAnalyticsSDK" version="1.3.0"/>
	        </group>
	        <group targetFramework="win8">
	            <dependency id="GoogleAnalyticsSDK" version="1.3.0"/>
	        </group>
		</dependencies>
    </metadata>
    <files>
