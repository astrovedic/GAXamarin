        <file src="lib/Plugin.GAXamarin.Abstractions.dll" target="lib/portable-net45+netcore45+wpa81+wp8+MonoAndroid10+Xamarin.iOS10/Plugin.GAXamarin.Abstractions.dll" />
        <file src="lib/Plugin.GAXamarin.dll" target="lib/portable-net45+netcore45+wpa81+wp8+MonoAndroid10+Xamarin.iOS10/Plugin.GAXamarin.dll" />

        <!-- Xamarin.iOS (Unified)-->
